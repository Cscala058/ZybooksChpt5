import java.util.Scanner;

public class GrayRemoverRGB {
   public static void main(String[] args) {
      /* Type your code here. */
         int r, g, b;
   Scanner sc = new Scanner(System.in);
   r = sc.nextInt();
   g = sc.nextInt();
   b = sc.nextInt();
   
   int small = (r < g && r < b)? r : (g < b)? g : b;
   
   System.out.println(r-small+" "+(g-small)+" "+(b-small));
   
   }
}
