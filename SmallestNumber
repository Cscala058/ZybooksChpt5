import java.util.Scanner;
public class SmallestNumber {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int a, b, c;
        a = scan.nextInt();
        b = scan.nextInt();
        c = scan.nextInt();

        int min = a;
        if(min > b){
            min = b;
        }
        if(min > c){
            min = c;
        }
        System.out.println(min);
    }
}
